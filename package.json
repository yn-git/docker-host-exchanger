{
	"name": "docker-host-exchanger",
	"displayName": "docker host exchanger",
	"publisher": "ynny",
	"description": "This extension can change docker.host quickly.",
	"version": "0.0.1",
	"license": "MIT",
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:docker-host-exchanger.change"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "docker-host-exchanger.change",
				"title": "dhe: change"
			}
		],
		"configuration": {
			"type": "object",
			"title": "dhe(Docker Host Exchanger) configuration",
			"properties": {
				"dhe.hostList": {
					"description": "Docker host list you want to access.",
					"type": "object",
					"default": {},
					"scope": "machine"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vsce": "^1.83.0",
		"vscode-test": "^1.4.1"
	}
}
